@using WebApplication2.Models;
@model List<Hospital>

@{
    ViewData["title"] = "Hospital";
}

@if (Model.Count != 0)
{
    <table class="table table-dark">
        <thead>
            <tr>
                <th scope="col">id</th>
                <th scope="col">name</th>
                <th scope="col">address</th>
                <th scope="col">type</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var e in Model)
            {
                <tr>
                    <td>@e.Id</td>
                    <td>@e.HospitalName</td>
                    <td>@e.Address</td>
                    <td>@e.Type</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <div class="card">
        <div class="card-body">
            No hay datos.
        </div>
    </div>
}
